{
  "name": "mini-search-engine",
  "version": "1.0.0",
  "description": "C++ based search engine for DSA problems with web interface",
  "main": "web-interface/server.js",
  "scripts": {
    "build": "g++ -o search_engine Mini_Search_Engine_CLI.cpp -std=c++11",
    "start": "npm run build && cd web-interface && npm install && node server.js",
    "dev": "cd web-interface && node server.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2"
  },
  "keywords": [
    "search-engine",
    "cpp",
    "data-structures",
    "dsa",
    "algorithms"
  ],
  "author": "Pritham Nandavaram",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/mini-search-engine.git"
  }
}